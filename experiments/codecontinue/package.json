{
  "name": "codecontinue",
  "displayName": "codeContinue",
  "description": "LLM powered code completion tool",
  "version": "0.0.1",
  "publisher": "kXborg",
  "repository": {
    "type": "git",
    "url": "https://github.com/kXborg/codeContinue"
  },
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Machine Learning",
    "Programming Languages"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codecontinue.configure",
        "title": "CodeContinue: Configure"
      },
      {
        "command": "codecontinue.suggest",
        "title": "CodeContinue: Suggest"
      },
      {
        "command": "codecontinue.clearSuggestion",
        "title": "CodeContinue: Clear Suggestion"
      },
      {
        "command": "codecontinue.showLogs",
        "title": "CodeContinue: Show Logs"
      }
    ],
    "configuration": {
      "title": "CodeContinue",
      "properties": {
        "codeContinue.endpoint": {
          "type": "string",
          "default": "",
          "description": "OpenAI-compatible API endpoint URL (e.g., https://api.openai.com/v1/chat/completions)"
        },
        "codeContinue.model": {
          "type": "string",
          "default": "gpt-3.5-turbo",
          "description": "Model name to use for completions"
        },
        "codeContinue.apiKey": {
          "type": "string",
          "default": "",
          "description": "API key for authentication (optional, leave empty if not required)"
        },
        "codeContinue.maxContextLines": {
          "type": "number",
          "default": 40,
          "minimum": 10,
          "maximum": 200,
          "description": "Maximum number of lines to send as context (before + after cursor)"
        },
        "codeContinue.timeoutMs": {
          "type": "number",
          "default": 10000,
          "minimum": 1000,
          "maximum": 60000,
          "description": "API request timeout in milliseconds (default: 10000ms = 10 seconds)"
        },
        "codeContinue.triggerLanguages": {
          "type": "array",
          "default": [
            "python",
            "javascript",
            "typescript",
            "javascriptreact",
            "typescriptreact",
            "cpp",
            "c",
            "java",
            "go",
            "rust",
            "php",
            "ruby",
            "swift",
            "kotlin",
            "csharp"
          ],
          "description": "Programming languages that trigger auto-completion on Enter key"
        },
        "codeContinue.rateLimitMs": {
          "type": "number",
          "default": 1000,
          "minimum": 100,
          "maximum": 10000,
          "description": "Minimum time between requests in milliseconds (prevents spam)"
        },
        "codeContinue.enableLogging": {
          "type": "boolean",
          "default": false,
          "description": "Enable debug logging to output channel"
        },
        "codeContinue.temperature": {
          "type": "number",
          "default": 0.3,
          "minimum": 0,
          "maximum": 2,
          "description": "LLM temperature (0 = deterministic, 2 = creative)"
        },
        "codeContinue.maxTokens": {
          "type": "number",
          "default": 1024,
          "minimum": 64,
          "maximum": 4096,
          "description": "Maximum tokens in completion response"
        },
        "codeContinue.disableDefaultInlineSuggest": {
          "type": "boolean",
          "default": true,
          "description": "Disable VSCode's default inline suggestions when CodeContinue is active (recommended to avoid conflicts)"
        }
      }
    },
    "keybindings": [
      {
        "command": "codecontinue.suggest",
        "key": "ctrl+enter",
        "mac": "cmd+enter",
        "when": "editorTextFocus"
      },
      {
        "command": "codecontinue.clearSuggestion",
        "key": "escape",
        "when": "editorTextFocus && inlineSuggestionVisible"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "esbuild": "^0.27.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}